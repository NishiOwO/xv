all:: docs/xv.$(MANNEWSUFFIX)

docs/xv.$(MANNEWSUFFIX): docs/xv.$(MANSRCSUFFIX)
	$(RM) $@
	if test -z "$(PREPROCESSMANPAGES)" ; then \
	   cd `dirname docs/xv` && \
	   $(LN) `basename docs/xv.$(MANSRCSUFFIX)` `basename $@`; \
	else \
	      	     $(RAWCPP)  $(MANDEFS)  $(EXTRAMANDEFS) \
	     < docs/xv.$(MANSRCSUFFIX) | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@; \
	fi

cleandir::
	$(RM) docs/xv.$(MANNEWSUFFIX)

all:: docs/xv.0

docs/xv.0:: docs/xv.$(MANNEWSUFFIX)
	@if [ -f docs/xv.$(MANNEWSUFFIX) ]; \
	then \
	cat docs/xv.$(MANNEWSUFFIX) | $(NEQN) | $(TBL) | $(NROFF) $(MANMACROS) | $(COL) > docs/xv.0; \
	fi

